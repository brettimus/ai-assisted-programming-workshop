{
  "name": "ai-assisted-programming-workshop",
  "version": "1.0.0",
  "description": "Terminal-based slide deck for AI Assisted Programming presentation",
  "main": "dist/index.js",
  "bin": {
    "present": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "present": "node dist/index.js",
    "lint": "biome lint src/",
    "format": "biome format src/ --write",
    "check": "biome check src/",
    "fix": "biome check src/ --apply"
  },
  "keywords": [
    "presentation",
    "slides",
    "terminal",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "4",
    "commander": "^14.0.0",
    "figlet": "1",
    "inquirer": "^12.6.1"
  }
}